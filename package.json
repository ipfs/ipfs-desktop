{
  "name": "ipfs-desktop",
  "private": true,
  "version": "1.0.0",
  "productName": "IPFS Desktop",
  "description": "IPFS Native Application",
  "main": "out/index.js",
  "scripts": {
    "start": "electron -r @babel/register src/index.js",
    "lint": "standard",
    "postinstall": "electron-builder install-app-deps",
    "build": "run-s build:*",
    "build:webui": "cross-env CID=QmQYAwCEy1wAxXwkQBzeA1L7RXjmm7Sk3T6mQAHvwTFxpj npm run build:webui:with-cid",
    "build:webui:with-cid": "cross-env-shell \"shx test -d src/utils/webui/app/ || (npm run build:webui:dir && (npm run build:webui:fetch-ipfs || npm run build:webui:fetch-http))\"",
    "build:webui:dir": "shx mkdir -p src/utils/webui/app",
    "build:webui:fetch-ipfs": "cross-env-shell \"ipfs get $CID -o src/utils/webui/app/\"",
    "build:webui:fetch-http": "cross-env-shell \"node scripts/fetch-webui-from-gateway.js $CID src/utils/webui/app/\"",
    "build:icons": "svgr -d src/icons node_modules/ipfs-css/icons && standard --fix src/icons/*.js",
    "build:babel": "babel src --out-dir out --copy-files",
    "build:binaries": "electron-builder -p never"
  },
  "pre-commit": [
    "lint"
  ],
  "keywords": [
    "ipfs",
    "electron",
    "station",
    "desktop",
    "app"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ipfs-shipyard/ipfs-desktop"
  },
  "author": "IPFS",
  "contributors": [
    "Kristoffer Str√∂m <kristoffer@rymdkoloni.se>",
    "David Dias <daviddias@ipfs.io>",
    "Juan Benet <juan@ipfs.io>",
    "Friedel Ziegelmayer <dignifiedquire@gmail.com>",
    "Henrique Dias <hacdias@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ipfs-shipyard/ipfs-desktop/issues"
  },
  "homepage": "https://github.com/ipfs-shipyard/ipfs-desktop",
  "build": {
    "appId": "io.ipfs.desktop",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "nsis": {
      "oneClick": false
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@svgr/cli": "^3.0.0",
    "cross-env": "^5.2.0",
    "electron": "^3.0.2",
    "electron-builder": "^20.28.4",
    "npm-run-all": "^4.1.3",
    "pre-commit": "^1.2.2",
    "request": "^2.88.0",
    "request-progress": "^3.0.0",
    "shx": "^0.3.2",
    "standard": "^12.0.1",
    "tar": "^4.4.6"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "auto-launch": "^5.0.5",
    "electron-menubar": "^1.0.1",
    "electron-serve": "^0.2.0",
    "electron-store": "^2.0.0",
    "fs-extra": "^7.0.0",
    "go-ipfs-dep": "^0.4.17",
    "ipfs": "^0.32.3",
    "ipfs-api": "^24.0.2",
    "ipfs-css": "^0.11.0",
    "ipfsd-ctl": "^0.39.2",
    "is-ipfs": "^0.4.7",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "shortid": "^2.2.13",
    "tachyons": "^4.11.1",
    "winston": "^3.1.0"
  }
}
