{
  "name": "ipfs-desktop",
  "private": true,
  "version": "0.7.0-rc.3",
  "productName": "IPFS Desktop",
  "description": "IPFS Native Application",
  "main": "out/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron -r @babel/register src/index.js",
    "lint": "standard",
    "test": "cross-env NODE_ENV=test mocha",
    "postinstall": "run-s install-app-deps build:webui",
    "install-app-deps": "electron-builder install-app-deps",
    "clean:webui": "shx rm -rf src/lib/webui/app/",
    "build": "run-s build:*",
    "build:webui": "run-s build:webui:*",
    "build:webui:download": "npx ipfs-or-gateway -c Qmb6o8HKrouK2WXVFS8753BArqnx5xz68tk6779ncPxpd8 -p src/lib/webui/app/",
    "build:webui:minimize": "shx rm -rf src/lib/webui/app/static/js/*.map && shx rm -rf src/lib/webui/app/static/css/*.map",
    "build:babel": "babel src --ignore src/lib/webui/app --out-dir out --copy-files",
    "build:binaries": "electron-builder"
  },
  "pre-commit": [
    "lint"
  ],
  "keywords": [
    "ipfs",
    "station",
    "desktop",
    "app"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ipfs-shipyard/ipfs-desktop"
  },
  "author": "Protocol Labs, Inc.",
  "contributors": [
    "Kristoffer Str√∂m <kristoffer@rymdkoloni.se>",
    "David Dias <daviddias@ipfs.io>",
    "Juan Benet <juan@ipfs.io>",
    "Friedel Ziegelmayer <dignifiedquire@gmail.com>",
    "Henrique Dias <hacdias@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ipfs-shipyard/ipfs-desktop/issues"
  },
  "homepage": "https://github.com/ipfs-shipyard/ipfs-desktop",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@svgr/cli": "^4.1.0",
    "cross-env": "^5.2.0",
    "mocha": "^6.0.2",
    "electron": "^4.0.8",
    "electron-builder": "^20.39.0",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "request": "^2.88.0",
    "request-progress": "^3.0.0",
    "shx": "^0.3.2",
    "spectron": "^5.0.0",
    "standard": "^12.0.1",
    "tar": "^4.4.8",
    "tmp": "0.0.33"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.4",
    "auto-launch": "^5.0.5",
    "electron-serve": "^0.2.0",
    "electron-store": "^2.0.0",
    "electron-updater": "^4.0.6",
    "filesize": "^4.1.2",
    "fs-extra": "^7.0.1",
    "go-ipfs-dep": "0.4.18",
    "i18next": "^15.0.6",
    "i18next-electron-language-detector": "0.0.10",
    "i18next-icu": "^1.1.2",
    "i18next-node-fs-backend": "^2.1.1",
    "ipfsd-ctl": "^0.42.0",
    "is-ipfs": "^0.6.0",
    "winston": "^3.2.1"
  }
}
